{
  "name": "outline-eclipsed",
  "displayName": "Outline Eclipsed",
  "description": "An outline view for VS Code that lets you drag and drop to reorganize your document structure.",
  "version": "0.3.3",
  "publisher": "douglashellinger",
  "icon": "icon.png",
  "author": {
    "name": "Douglas Hellinger"
  },
  "license": "MIT",
  "homepage": "https://github.com/doughgle/outline-eclipsed#readme",
  "bugs": {
    "url": "https://github.com/doughgle/outline-eclipsed/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doughgle/outline-eclipsed.git"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/doughgle"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "outline",
    "markdown",
    "drag-drop",
    "reorder",
    "tree-view",
    "navigation"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "outlineEclipsed",
          "name": "Outline Eclipsed",
          "icon": "icon.png"
        }
      ]
    },
    "commands": [
      {
        "command": "outlineEclipsed.gotoItem",
        "title": "Go to Outline Item"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "clean": "rm -rf out",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  }
}
